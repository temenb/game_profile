generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id        String   @id @default(uuid())
  userId    String   @unique
  nickname  String
  level     Int      @default(1)
  rating    Int      @default(1000)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
